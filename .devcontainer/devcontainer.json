{
  "dockerComposeFile": "../docker-compose.yml",
  "extensions": [
    "castwide.solargraph",
    "eamodio.gitlens",
    "kaiwood.endwise",
    "noku.rails-run-spec-vscode",
    "misogi.ruby-rubocop"
  ],
  "postCreateCommand": "bundle && yard gems",
  "service": "app",
  "workspaceFolder": "/app",
  "settings": {
    "solargraph.useBundler": true,
    "solargraph.bundlerPath": "bundle"
  }
}
